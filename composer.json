{
    "name": "numaxlab/lunar-geslib",
    "type": "library",
    "version": "1.0.0-beta.1",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-zip": "*",
        "illuminate/support": "*",
        "livewire/livewire": "^3.6",
        "lunarphp/core": "^1.0.0",
        "lunarphp/lunar": "^1.0.0",
        "numaxlab/atomic-laravel": "dev-main",
        "numaxlab/geslib-files": "^1.0@beta",
        "numaxlab/dilve-client": "@dev",
        "spatie/array-to-xml": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "orchestra/testbench": "^10.4",
        "laravel/pint": "dev-main",
        "peckphp/peck": "dev-main",
        "larastan/larastan": "^3.0",
        "rector/rector": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "NumaxLab\\Lunar\\Geslib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "./vendor/bin/phpstan analyse -c phpstan.neon --memory-limit=512M",
        "test:typos": "./vendor/bin/peck",
        "test:lint": "./vendor/bin/pint --test",
        "test:types": "@phpstan",
        "test:refactor": "./vendor/bin/rector process --dry-run",
        "test:unit": "./vendor/bin/pest",
        "test": [
            "@test:typos",
            "@test:types",
            "@test:lint",
            "@test:refactor",
            "@test:unit"
        ],
        "refactor": "./vendor/bin/rector process",
        "fix": [
            "@refactor",
            "./vendor/bin/pint"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "NumaxLab\\Lunar\\Geslib\\LunarGeslibServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Adrián Pardellas Blunier",
            "email": "adrian@numax.org"
        },
        {
            "name": "Noa García Amado",
            "email": "noa@numax.org"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
